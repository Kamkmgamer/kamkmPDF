{
  "name": "pdfprompt",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "next build --turbo",
    "check": "eslint . && tsc --noEmit",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "drizzle-kit migrate",
    "db:push": "drizzle-kit push",
    "db:studio": "drizzle-kit studio",
    "dev": "next dev --turbo",
    "worker": "tsx --env-file=.env ./src/server/jobs/worker.ts",
    "worker:dev": "tsx watch --env-file=.env ./src/server/jobs/worker.ts",
    "worker:prod": "node ./dist/server/jobs/worker.js",
    "format:check": "prettier --check \"**/*.{ts,tsx,js,jsx,mdx}\" --cache",
    "format:write": "prettier --write \"**/*.{ts,tsx,js,jsx,mdx}\" --cache",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "preview": "next build && next start",
    "start": "next start",
    "typecheck": "tsc --noEmit",
    "test": "vitest",
    "prepare": "husky"
  },
  "dependencies": {
    "@clerk/nextjs": "^6.33.0",
    "@polar-sh/nextjs": "^0.4.9",
    "@polar-sh/sdk": "^0.12.0",
    "@react-email/components": "^0.5.5",
    "@sparticuz/chromium": "^132.0.0",
    "@t3-oss/env-nextjs": "^0.12.0",
    "@tanstack/react-query": "^5.90.2",
    "@trpc/client": "^11.6.0",
    "@trpc/react-query": "^11.6.0",
    "@trpc/server": "^11.6.0",
    "@uploadthing/react": "^7.3.3",
    "date-fns": "^4.1.0",
    "drizzle-orm": "^0.41.0",
    "framer-motion": "^12.23.22",
    "lucide-react": "^0.544.0",
    "next": "^15.5.4",
    "node-fetch": "^3.3.2",
    "pdfjs-dist": "^5.4.149",
    "pdfkit": "^0.17.2",
    "pino": "^9.12.0",
    "pino-pretty": "^13.1.1",
    "postgres": "^3.4.7",
    "puppeteer": "^24.22.3",
    "puppeteer-core": "^24.22.3",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-pdf": "^10.1.0",
    "recharts": "^3.2.1",
    "resend": "^6.1.1",
    "server-only": "^0.0.1",
    "superjson": "^2.2.2",
    "uploadthing": "^7.7.4",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.1",
    "@tailwindcss/postcss": "^4.1.13",
    "@testing-library/jest-dom": "^6.8.0",
    "@types/jest": "^30.0.0",
    "@types/node": "^20.19.17",
    "@types/pdfkit": "^0.17.3",
    "@types/react": "^19.1.14",
    "@types/react-dom": "^19.1.9",
    "dotenv": "^17.2.3",
    "drizzle-kit": "^0.30.6",
    "eslint": "^9.36.0",
    "eslint-config-next": "^15.5.4",
    "eslint-plugin-drizzle": "^0.2.3",
    "husky": "^9.1.7",
    "jsdom": "^27.0.0",
    "lint-staged": "^16.2.1",
    "postcss": "^8.5.6",
    "prettier": "^3.6.2",
    "prettier-plugin-tailwindcss": "^0.6.14",
    "tailwindcss": "^4.1.13",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "tsx": "^4.20.6",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.44.1",
    "vite-tsconfig-paths": "^5.1.4",
